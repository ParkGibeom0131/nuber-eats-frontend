{
  "name": "nuber-eats-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/google-map-react": "^2.1.7",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.39",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "apollo": "^2.34.0",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-testing-library": "^6.0.0",
    "google-map-react": "^2.2.1",
    "graphql": "^16.7.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.0",
    "http-proxy-middleware": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.45.2",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "victory": "^36.6.11",
    "web-vitals": "^2.1.4",
    "ws": "^8.13.0"
  },
  "scripts": {
    "tailwind:prodbuild": "cross-env NODE_ENV=production npm run tailwind:build",
    "tailwind:build": "tailwind build ./src/styles/tailwind.css -o ./src/styles/styles.css",
    "start": "npm run generate & npm run tailwind:build & react-scripts start",
    "apollo:codegen": "rimraf src/__generated__ && client:codegen src/__generated__ --target=typescript --outputFlat",
    "generate": "graphql-codegen",
    "build": "CI=false react-scripts build",
    "prebuild": "npm run tailwind:prodbuild",
    "test": "react-scripts test --verbose",
    "test:cov": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@types/googlemaps": "^3.43.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.47.0",
    "mock-apollo-client": "^1.2.1",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/components/**/*.tsx",
      "./src/pages/**/*.tsx",
      "./src/routers/**/*.tsx"
    ]
  }
}
